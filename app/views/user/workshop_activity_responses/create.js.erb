<% if @workshop_activity_response.errors.empty? %>
	var id = "#activity-<%= @workshop_activity_response.workshop_activity_id %>";
	if ($(id + " > .alert").length > 0) {
		$(id + " > .alert").remove();	
	}
	$(id).append("<%= j (render partial:'user/workshops/show_student/activity_response',locals: {response:@workshop_activity_response}) %>");
	$(".ui-dialog-titlebar-close").click();
	photoView();
<% end %>