<% if @workshop_activity_response.errors.empty? %>
	var idElm = "#workshop-activity-response-<%= @workshop_activity_response.id %>";
	$(idElm).remove();
	flashNoticeMessage("<%= t('messages.destroyed_record') %>");
<% else %>
	flashWarningMessage("<%= @workshop_activity_response.errors.full_messages.to_sentence %>");
<% end %>
