<h4>
	<%= t('.vacancies_users') %>
</h4>
<ul class="workshop-participants">
	<% @workshop.participants.each do |participant| %>
		<%= content_tag :li, id:"participant-#{@workshop.id}#{participant.id}" do %>
		  <%= image_tag participant.profile.avatar.url(:thumb),class:'user-avatar' %>
		  <%= link_to participant.profile.user_name,
		              user_photographer_path(participant.id),
		              class:"photographer-profile user-name" %>
		<% end %>
	<% end %>
</ul>