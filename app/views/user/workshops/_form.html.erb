<%= semantic_form_for @workshop do |f| %>
  <%= f.inputs do %>
    <%= f.input :user %>
    <%= f.input :description %>
    <%= f.input :details %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :vacancies_number %>
    <%= f.input :value %>
    <%= f.input :prerequisite %>
    <%= f.input :goal %>
    <%= f.input :target_audience %>
    <%= f.input :term %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
